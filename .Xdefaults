!first is primary and the rest are for glyph fallthrough
urxvt.font: xft:Hack-9, \
            xft: Hack-11, \
            xft: Monospace-11
urxvt*skipBuiltinGlyphs: 1
URxvt.cursorBlink:       1
URxvt.cursorUnderline:   1

urxvt.saveLines:       20000
urxvt*scrollBar:       0
urxvt*scrollBar_right: 1
urxvt*scrollColor:     #000000

urxvt*fading:          5

!select
URxvt.keysym.M-s: perl:keyboard-select:activate
URxvt.keysym.M-r: perl:keyboard-select:search
